{
  "name": "hsk-ai-coach",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "dev:web": "turbo dev --filter=@hsk/web",
    "dev:api": "turbo dev --filter=@hsk/api",
    "db:generate": "pnpm --filter @hsk/api db:generate",
    "db:push": "pnpm --filter @hsk/api db:push",
    "db:migrate": "pnpm --filter @hsk/api db:migrate",
    "db:studio": "pnpm --filter @hsk/api db:studio"
  },
  "devDependencies": {
    "turbo": "^2.3.0"
  },
  "packageManager": "pnpm@9.15.0"
}
